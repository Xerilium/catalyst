{
  "integrations": [
    {
      "name": "Claude",
      "commands": {
        "path": ".claude/commands",
        "useNamespaces": true,
        "separator": ":",
        "useFrontMatter": true,
        "extension": "md"
      }
    },
    {
      "name": "GitHub Copilot",
      "commands": {
        "path": ".github/prompts",
        "useNamespaces": false,
        "separator": ".",
        "useFrontMatter": false,
        "extension": "prompt.md"
      }
    },
    {
      "name": "Cursor",
      "commands": {
        "path": ".cursor/commands",
        "useNamespaces": true,
        "separator": "/",
        "useFrontMatter": true,
        "extension": "md"
      }
    }
  ]
}